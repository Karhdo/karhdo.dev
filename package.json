{
  "name": "starter-blog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "postinstall": "npx prisma generate",
    "build": "cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node -r esbuild-register ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "migrate:postgres": "dotenv -e .env.local -- npx prisma migrate deploy"
  },
  "dependencies": {
    "@next/bundle-analyzer": "13.1.6",
    "@next/font": "13.1.6",
    "@octokit/graphql": "^7.0.1",
    "@prisma/client": "^5.3.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.195",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "contentlayer": "0.3.0",
    "esbuild": "0.17.19",
    "github-slugger": "^1.4.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "isomorphic-unfetch": "^4.0.2",
    "lucide-react": "^0.268.0",
    "mdx-bundler": "^9.0.0",
    "next": "13.1.6",
    "next-contentlayer": "0.3.0",
    "next-themes": "^0.2.0",
    "pliny": "0.0.10",
    "postcss": "^8.4.16",
    "prisma": "^5.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-snowfall": "^1.2.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.5.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.0.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "swr": "^2.2.0",
    "tailwindcss": "^3.2.2",
    "typed.js": "^2.0.16",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "esbuild-register": "3.4.2",
    "eslint": "^8.23.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "husky": "^8.0.0",
    "js-yaml": "4.1.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "typescript": "^4.8.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
